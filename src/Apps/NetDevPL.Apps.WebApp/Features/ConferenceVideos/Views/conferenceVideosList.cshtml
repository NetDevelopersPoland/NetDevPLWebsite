@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<NetDevPLWeb.Features.ConferenceVideos.ConferenceVideosViewModel>

<div class="row equalize-row">
    @foreach (var webcast in Model.ConferenceVideosList)
    {
        <div class="col-xs-6 col-md-3 equalize">
            <div class="thumbnail">
                <div class="embed-responsive embed-responsive-16by9">
                    <iframe class="embed-responsive-item" src="@webcast.Url" frameborder="0" allowfullscreen></iframe>
                </div>
                <div class="caption">
                    <h3>@webcast.Title</h3>
                    <p>@webcast.Description</p>
                </div>
            </div>
        </div>
    }
</div>

