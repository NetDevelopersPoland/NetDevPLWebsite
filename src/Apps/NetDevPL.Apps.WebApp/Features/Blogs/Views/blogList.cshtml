@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<NetDevPLWeb.Features.Blogs.BlogListViewModel>

<div class="jumbotron">
    <h2>Lista blogów</h2>

    <ul>
        @foreach (var blog in Model.Blogs)
        {
            <li><a href="@blog.Url" target="_blank">@blog.Title</a></li>
        }
    </ul>
</div>
